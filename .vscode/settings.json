{
    "cSpell.language": "en",
    "cSpell.words": [
        "autoencoder",
        "autoencoders",
        "microarray",
        "proteomic",
        "upregulated"
    ],
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.quickSuggestions": false,
        "editor.tabSize": 2
    },
    "python.venvPath": "~/miniconda3/envs",
    "python.envFile": "~/miniconda3/envs/py36",
    "python.pythonPath": "~/miniconda3/envs/py36/bin/python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true
    },
    "python.autoComplete.preloadModules": [
        "tensorflow",
        "numpy",
        "pandas"
    ]
}
